{
  "name": "black-box",
  "displayName": "Black Box",
  "description": "Real-time developer problem solver",
  "publisher": "nikhilmjeby",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:black-box.findDocs",
    "onCommand:black-box.askQuestion",
    "onCommand:black-box.liveDocumentation",
    "onCommand:black-box.sendMessage",
    "onCommand:black-box.openLiveDoc",
    "onCommand:black-box.refreshLiveDoc"
  ],
  "main": "./extension.js",
  "extensionDependencies": [
    "vscode.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "black-box.findDocs",
        "category": "BlackBox",
        "title": "find documentation"
      },
      {
        "command": "black-box.askQuestion",
        "category": "BlackBox",
        "title": "Ask me a question"
      },
      {
        "command": "black-box.liveDocumentation",
        "category": "BlackBox",
        "title": "Open Live Documentation"
      },
      {
        "command": "black-box.sendMessage",
        "category": "BlackBox",
        "title": "sendMessage"
      }
    ],
    "keybindings": [
      {
        "command": "black-box.openLiveDoc",
        "key": "ctrl+m",
        "mac": "cmd+m"
      },
      {
        "command": "black-box.refreshLiveDoc",
        "key": "alt+m",
        "mac": "alt+m"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.5.1",
    "node-fetch": "^2.6.6"
  }
}
